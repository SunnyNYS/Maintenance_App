                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textStyle="bold"
                        android:textColor="@color/textViewColor"
                        android:text="Tone Frequency (Clearance)" />

                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyCLR150"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="150 Hz"
                        android:textColor="@color/textViewColor" />

                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyTransfer"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="Transfer from System A to System B (Nominal 1 sec)"
                        android:textColor="@color/textViewColor" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/textViewGpAsiiMonthlyP1Tx1"
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/teal_700"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:text="TX-1" />


                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx1CRS90"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="58dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp" />

                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx1CRS150"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="5dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp"/>


                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx1CLR150"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="58dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/textViewGpAsiiMonthlyP1Tx2"
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/teal_700"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:text="TX-2" />


                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx2CRS90"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="58dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp" />

                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx2CRS150"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="5dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp"/>

                            <EditText
                                android:id="@+id/editTextGpAsiiMonthlyTx2CLR150"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:layout_marginTop="58dp"
                                android:ems="10"
                                android:hint="±1.0 %"
                                android:minHeight="48dp"
                                android:textSize="14sp"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal">
                        <Switch
                            android:id="@+id/switchGpAsiiMonthlyTransfer"
                            android:layout_width="wrap_content"
                            android:layout_height="40dp"
                            android:textOn="OK"
                            android:textOff="Not Ok"
                            android:text="Not OK "
                            tools:ignore="TouchTargetSizeCheck"/>
                        <TextView
                            android:id="@+id/textViewGpAsiiMonthlyTransferOk"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ems="3"
                            android:text="OK"
                            android:textColor="@color/black"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlySNo2"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:textColor="@color/teal_700"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="5."
                        android:layout_weight="0.2"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthly12"
                        android:layout_width="wrap_content"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="5.1"/>

                    <TextView
                        android:id="@+id/textViewGpAsiiMonthly13"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="5.2" />
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthly14"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="5.3" />
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthly15"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="5.4" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyPara2"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/teal_700"
                        android:text="System Operation On Battery Back up"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyBattBD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="Battery Voltage Before Discharge (V)" />
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyBattOps10"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="Operation on batteries for 10 min (approx.)"
                        android:textColor="@color/textViewColor" />
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyBattAD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="Battery Voltage after Discharge (V)"
                        android:textColor="@color/textViewColor" />
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyTerClean"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="Terminals Cleaned/properly Tightened" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyTx1"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/teal_700"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:text="TX-1" />


                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx1BattBD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx1BattOps10"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx1BattAD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx1TerClean"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_weight="1.1"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewGpAsiiMonthlyTx2"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/teal_700"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:text="TX-2" />


                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx2BattBD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx2BattOps10"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx2BattAD"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
                        android:hint=""
                        android:minHeight="48dp"
                        android:textSize="14sp" />

                    <EditText
                        android:id="@+id/editTextGpAsiiMonthlyTx2TerClean"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="10"
