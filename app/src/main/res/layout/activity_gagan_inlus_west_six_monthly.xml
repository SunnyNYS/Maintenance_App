                    <EditText
                        android:id="@+id/editTextInlusWestSixE18"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="&lt; 5 Ohms"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextInlusWestSixE19"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="&lt; 5 Ohms"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextInlusWestSixE20"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="&lt; 5 Ohms"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextInlusWestSixE20A"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="&lt; 5 Ohms"
                        android:textSize="14sp"/>
