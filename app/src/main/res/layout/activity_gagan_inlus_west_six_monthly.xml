                        android:id="@+id/textViewInlusWestSixTol3"
                        android:layout_width="match_parent"
                        android:layout_height="58dp"
                        android:layout_marginTop="40dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="Relay changes over and equipment operates normal" />
                    <TextView
                        android:id="@+id/textViewInlusWestSixTol4"
                        android:layout_width="match_parent"
                        android:layout_height="58dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/textViewColor"
                        android:text="PCU Works Normal" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="5dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewInlusWestSixMeasured1"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/teal_700"
                        android:text="Observed (Status/Value)"
                        android:gravity="center"
                        android:textStyle="bold"/>
                    <EditText
                        android:id="@+id/editTextInlusWestSixER"
                        android:layout_width="match_parent"
                        android:layout_height="58dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="&lt; 5 ohms"
                        android:textSize="14sp"/>
                    <EditText
                        android:id="@+id/editTextInlusWestSixBrazing"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:ems="6"
                        android:hint="Yes"
                        android:textSize="14sp"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">
                        <LinearLayout
