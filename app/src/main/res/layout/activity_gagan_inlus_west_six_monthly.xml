                                android:gravity="center"
                                android:text="PCU-A" />
                            <EditText
                                android:id="@+id/editTextInlusWestSixP1SwitchOff"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>
                            <EditText
                                android:id="@+id/editTextInlusWestSixP1SwitchON"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/textViewInlusWestSixPCU2"
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/textViewColor"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:text="PCU-B" />
                            <EditText
                                android:id="@+id/editTextInlusWestSixP2SwitchOff"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>
                            <EditText
                                android:id="@+id/editTextInlusWestSixP2SwitchON"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/textViewInlusWestSixPCU3"
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/textViewColor"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:text="PCU-C" />
                            <EditText
                                android:id="@+id/editTextInlusWestSixP3SwitchOff"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>
                            <EditText
                                android:id="@+id/editTextInlusWestSixP3SwitchON"
                                android:layout_width="match_parent"
                                android:layout_height="58dp"
                                android:layout_marginTop="5dp"
                                android:ems="6"
                                android:hint="Nml"
                                android:textSize="14sp"/>
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/textViewInlusWestSixTable2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:textSize="16sp"
                android:gravity="center"
                android:textColor="@color/teal_700"
                android:text="Table-1 INLUS-West Equipment Earth Pit Resistance"
                android:textStyle="bold"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textViewInlusWestSixSNo60"
                        android:layout_width="40dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="60dp"
                        android:gravity="center"
                        android:textColor="@color/textViewColor"
                        android:text="1"/>
                    <TextView
                        android:id="@+id/textViewInlusWestSixSNo61"
                        android:layout_width="40dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:textColor="@color/textViewColor"
                        android:text="2"/>
                    <TextView
                        android:id="@+id/textViewInlusWestSixSNo62"
                        android:layout_width="40dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
